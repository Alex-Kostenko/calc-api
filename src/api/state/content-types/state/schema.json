{
  "kind": "collectionType",
  "collectionName": "states",
  "info": {
    "singularName": "state",
    "pluralName": "states",
    "displayName": "State"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "port": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::port.port",
      "inversedBy": "states"
    },
    "locations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::location.location",
      "mappedBy": "state"
    }
  }
}
